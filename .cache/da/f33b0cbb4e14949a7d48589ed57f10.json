ext number\n    let newWheel = state.Wheel + amount; //The actual next number\n\n    //Finalize the next number:\n    if (newWheel < 0) {\n      newWheel = 5;\n    } else if (newWheel > 5) {\n      newWheel = 0;\n    }\n\n    //Update the data:\n    setData({\n      ...state,\n      TimesMoved: state.TimesMoved + 1,\n      Wheel: newWheel,\n    });\n  }\n\n  //render to the screen:\n  return (\n    <div className=\"WHEEL\">\n      <div className=\"wheel-container\">\n        {/* the actual wheel */}\n        <div id=\"wheel-physical\">\n          {[0, 1, 2, 3, 4, 5].map((val) => (\n            <div\n              key={val}\n              className={`cog ${state.Wheel === val ? \"active\" : \"\"}`}\n              style={{ \"--i\": val }}\n            >\n              {/* --i is a custom CSS property. */}\n            </div>\n          ))}\n        </div>\n        {/*--------     -----------*/}\n\n        <p className=\"TM_text\">{`Times moved: ${state.TimesMoved}`}</p>\n\n        {/* KEYPAD */}\n        <div id=\"keypad\">\n          <button onClick={() => onClick(\"-\")}>{\"-\"}</button>\n          <button onClick={() => onClick(\"+\")}>{\"+\"}</button>\n        </div>\n        {/*--------     -----------*/}\n      </div>\n    </div>\n  );\n}\n//--\n//Used to verify if the user has logged in:\nfunction verify(navigate) {\n  const token = localStorage.getItem(\"token\"); //check for any active tokens\n\n  if (token == null) {\n    //IF the user is not logged in,\n    navigate(\"/\"); //redirect.\n  }\n}\n\n//\n//\n//----Exports:\nexport default Wheel;\n"},"lineCount":null}},"error":null,"hash":"149ad5116b72df838cb2b7f144540bee","cacheData":{"env":{}}}